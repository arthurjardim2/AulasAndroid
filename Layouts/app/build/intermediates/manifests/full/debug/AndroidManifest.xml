<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.josias.layouts"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.android.josias.layouts.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.josias.layouts.LinearLayoutActivity"
            android:label="@string/title_activity_linear_layout" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.RelativeLayoutActivity"
            android:label="@string/title_activity_relative_layout" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.FrameLayoutActivity"
            android:label="@string/title_activity_frame_layout" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.DrawerLayoutActivity"
            android:label="@string/title_activity_drawer_layout" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.ViewsActivity"
            android:label="@string/title_activity_views" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.views.TextViewEditTextButtonActivity"
            android:label="@string/title_activity_tv_ed_button" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.views.CheckBoxActivity"
            android:label="@string/title_activity_check_box" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.views.RadioButtonActivity"
            android:label="@string/title_activity_radio_button" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.views.ImageViewActivity"
            android:label="@string/title_activity_image_view" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.views.SpinnerActivity"
            android:label="@string/title_activity_spinner" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.views.RatingBarActivity"
            android:label="@string/title_activity_rating_bar" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.views.WebViewActivity"
            android:label="@string/title_activity_web_view" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.views.DateTimePickerActivity"
            android:label="@string/title_activity_date_time_picker" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.DialogsActivity"
            android:label="@string/title_activity_dialogs" >
        </activity>
        <activity
            android:name="com.android.josias.layouts.views.MenusActivity"
            android:label="@string/title_activity_menus" >
        </activity>
    </application>

</manifest>